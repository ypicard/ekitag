{
  "manifest_version": 3,
  "name": "EkiTag",
  "description": "This extension directly adds match results to Eki's impressive database.",
  "version": "1.2.1",
  "browser_action":
  {
    "default_icon": "ekitag.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
